{
  "presets": [
    ["env", {
      "targets": {
        "browsers": ["last 2 Firefox version", "last 2 Chrome versions"],
        "uglify": true
      },
      "modules": false
    }],
    "stage-2",
    "react"
  ],
  "plugins": [
    "transform-runtime",
    "syntax-dynamic-import"
  ],
  "env": {
    "test": {
      "presets": [
        ["env", { "targets": { "node": "current" }}],
        "stage-2",
        "react"
      ],
      "plugins": [
        "transform-runtime",
        "syntax-dynamic-import"
      ]
    }
  }
}