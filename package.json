{
  "name": "ysitd-cloud-console",
  "private": true,
  "version": "1.0.0",
  "description": "Console for YSITD Cloud",
  "main": "src/server.js",
  "repository": "git+https://github.com/ysitd-cloud/console.git",
  "author": "Tony Yip <tony@opensource.hk>",
  "license": "AGPL-3.0+",
  "scripts": {
    "start": "node src/backend/server.js",
    "dev": "nodemon src/backend/server.js --watch src/backend",
    "build": "node build/build.js",
    "lint": "eslint --ext .jsx,.js,.vue .",
    "setup:frontend": "webpack --config build/webpack.dev.conf.js",
    "setup:backend": "webpack --config build/webpack.server.conf.js",
    "test": "jest"
  },
  "dependencies": {
    "@webcomponents/custom-elements": "^1.0.8",
    "apollo-cache-inmemory": "^1.1.4",
    "apollo-client": "^2.0.4",
    "apollo-link-http": "^1.3.2",
    "axios": "^0.17.1",
    "babel-runtime": "^6.26.0",
    "body-parser": "^1.17.2",
    "chokidar": "^1.7.0",
    "classnames": "^2.2.5",
    "connect-redis": "^3.3.0",
    "dotenv": "^4.0.0",
    "express": "^4.15.4",
    "express-session": "^1.15.5",
    "graphql": "^0.11.7",
    "graphql-tag": "^2.6.1",
    "helmet": "^3.8.1",
    "moment": "^2.20.1",
    "morgan": "^1.9.0",
    "nunjucks": "^3.0.1",
    "passport": "^0.4.0",
    "passport-oauth2": "^1.4.0",
    "prop-types": "^15.6.0",
    "react": "^16.2.0",
    "react-apollo": "^2.0.4",
    "react-dom": "^16.2.0",
    "react-loadable": "^5.3.1",
    "serve-static": "^1.12.4",
    "vue": "^2.4.2",
    "vue-meta": "^1.1.0",
    "vue-router": "^3.0.0",
    "vue-server-renderer": "^2.4.2",
    "vuetify": "^0.17.6",
    "vuex": "^3.0.0",
    "vuex-router-sync": "^5.0.0"
  },
  "devDependencies": {
    "@vue/test-utils": "^1.0.0-beta.10",
    "assets-webpack-plugin": "^3.5.1",
    "babel-core": "^6.26.0",
    "babel-eslint": "^8.0.1",
    "babel-jest": "^22.1.0",
    "babel-loader": "^7.1.2",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-runtime": "^6.23.0",
    "babel-preset-env": "^1.6.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-2": "^6.24.1",
    "css-loader": "^0.28.5",
    "eslint": "^4.5.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-friendly-formatter": "^3.0.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-html": "^4.0.1",
    "eslint-plugin-import": "^2.7.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.6.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "friendly-errors-webpack-plugin": "^1.6.1",
    "jest": "^22.1.4",
    "jest-serializer-vue": "^0.3.0",
    "nodemon": "^1.12.1",
    "optimize-css-assets-webpack-plugin": "^3.1.1",
    "progress-bar-webpack-plugin": "^1.10.0",
    "react-test-renderer": "^16.2.0",
    "shelljs": "^0.7.8",
    "stylus": "^0.54.5",
    "stylus-loader": "^3.0.1",
    "vue-jest": "^1.4.0",
    "vue-loader": "^13.0.4",
    "vue-style-loader": "^3.0.1",
    "vue-template-compiler": "^2.4.2",
    "webpack": "^3.5.5",
    "webpack-dev-middleware": "^2.0.3",
    "webpack-hot-middleware": "^2.18.2",
    "webpack-merge": "^4.1.0",
    "webpack-node-externals": "^1.6.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "jsx",
      "vue"
    ],
    "moduleNameMapper": {
      "^@/(.*)$": "<rootDir>/src/$1"
    },
    "transform": {
      "^.+\\.js$": "<rootDir>/node_modules/babel-jest",
      "^.+\\.jsx$": "<rootDir>/node_modules/babel-jest",
      ".*\\.(vue)$": "<rootDir>/node_modules/vue-jest"
    },
    "snapshotSerializers": [
      "<rootDir>/node_modules/jest-serializer-vue"
    ],
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/frontend/**/*.{js,jsx,vue}",
      "!**/node_modules/**"
    ],
    "coverageReporters": [
      "html",
      "text-summary"
    ],
    "mapCoverage": true
  }
}
